{
    "name": "hexo-deployer-aws-s3",
    "version": "0.3.2",
    "description": "AWS S3 deployer for Hexo.",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/kei-ito/hexo-deployer-aws-s3.git"
    },
    "keywords": [
        "AWS",
        "S3",
        "Hexo"
    ],
    "author": {
        "name": "Kei Ito"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kei-ito/hexo-deployer-aws-s3/issues"
    },
    "scripts": {
        "lint": "eslint lib test",
        "test": "tap --coverage test/deployer.js test/s3.js",
        "version:changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file --release-count 0",
        "version:add": "git add .",
        "version": "npm-run-all --sequential version:changelog version:add"
    },
    "dependencies": {
        "aws-sdk": "^2.395.0",
        "bluebird": "^3.5.3",
        "glob": "^7.1.3",
        "mime": "^2.4.0",
        "upath": "^1.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.5.0",
        "@commitlint/config-conventional": "^7.5.0",
        "@nlib/lint": "^3.3.3",
        "conventional-changelog-cli": "^2.0.11",
        "eslint": "^5.13.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.3",
        "npm-run-all": "^4.1.5",
        "tap": "^12.5.1"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint"
        ]
    }
}
