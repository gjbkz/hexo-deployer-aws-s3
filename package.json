{
    "name": "hexo-deployer-aws-s3",
    "version": "1.1.1",
    "license": "MIT",
    "author": "Kei Ito <kei.itof@gmail.com>",
    "description": "AWS S3 deployer for Hexo.",
    "homepage": "https://github.com/kei-ito/hexo-deployer-aws-s3",
    "repository": "https://github.com/kei-ito/hexo-deployer-aws-s3",
    "engines": {
        "node": ">=12"
    },
    "main": "lib/index.js",
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "run-s build:*",
        "build:cleanup": "rimraf lib",
        "build:tsc": "tsc --project tsconfig.build.json",
        "test": "run-s test:setup test:ava",
        "test:setup": "run-s test:setup:*",
        "test:setup:cleanup": "rimraf test/project",
        "test:setup:hexo": "hexo init test/project",
        "test:setup:config": "ts-node test/setupConfig.ts",
        "test:ava": "ava",
        "lint": "eslint --ext .ts src test/*.ts",
        "version": "run-s version:changelog version:add",
        "version:changelog": "npx @nlib/changelog --output CHANGELOG.md",
        "version:add": "git add ."
    },
    "dependencies": {
        "@aws-sdk/client-s3": "3.131.0",
        "fast-glob": "3.2.11",
        "mime": "3.0.0",
        "upath": "2.0.1"
    },
    "devDependencies": {
        "@nlib/eslint-config": "3.18.0",
        "@nlib/githooks": "0.1.0",
        "@nlib/nodetool": "1.0.0",
        "@types/glob": "7.2.0",
        "@types/js-yaml": "4.0.5",
        "@types/mime": "2.0.3",
        "@types/mkdirp": "1.0.2",
        "@types/node": "17.0.45",
        "@typescript-eslint/eslint-plugin": "5.30.6",
        "@typescript-eslint/parser": "5.30.6",
        "ava": "4.3.1",
        "eslint": "8.20.0",
        "js-yaml": "4.1.0",
        "lint-staged": "13.0.3",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.2",
        "ts-node": "10.9.1",
        "typescript": "4.7.4"
    },
    "ava": {
        "timeout": "1m",
        "extensions": [
            "ts",
            "js"
        ],
        "require": [
            "ts-node/register"
        ],
        "files": [
            "test/test.ts"
        ]
    },
    "lint-staged": {
        "*.ts": [
            "eslint"
        ],
        "*.js": [
            "eslint"
        ]
    },
    "renovate": {
        "extends": [
            "github>nlibjs/renovate-config"
        ]
    }
}
