{
    "name": "hexo-deployer-aws-s3",
    "version": "1.0.2",
    "license": "MIT",
    "author": "Kei Ito <kei.itof@gmail.com>",
    "description": "AWS S3 deployer for Hexo.",
    "homepage": "https://github.com/kei-ito/hexo-deployer-aws-s3",
    "repository": "https://github.com/kei-ito/hexo-deployer-aws-s3",
    "engines": {
        "node": ">=12"
    },
    "main": "lib/index.js",
    "files": [
        "lib/*.js",
        "lib/*.d.ts"
    ],
    "scripts": {
        "build:cleanup": "rimraf lib",
        "build:tsc": "tsc",
        "build:sourceMap": "ts-node test/fixSourceMap.ts",
        "build": "run-s build:cleanup build:tsc build:sourceMap",
        "test:setup:cleanup": "rimraf test/project",
        "test:setup:hexo": "hexo init test/project",
        "test:setup:config": "ts-node test/setupConfig.ts",
        "test:setup": "run-s test:setup:cleanup test:setup:hexo test:setup:config",
        "test:core": "ava -v",
        "test": "run-s test:setup test:core",
        "lint": "eslint --ext .ts src test/*.ts",
        "version": "run-s version:changelog version:add",
        "version:changelog": "nlib-changelog --output CHANGELOG.md",
        "version:add": "git add ."
    },
    "dependencies": {
        "aws-sdk": "2.897.0",
        "globby": "11.0.3",
        "mime": "2.5.2",
        "upath": "2.0.1"
    },
    "devDependencies": {
        "@nlib/changelog": "0.1.9",
        "@nlib/eslint-config": "3.17.21",
        "@nlib/githooks": "0.0.5",
        "@nlib/lint-commit": "0.1.4",
        "@nlib/nodetool": "0.1.17",
        "@types/glob": "7.1.3",
        "@types/js-yaml": "4.0.1",
        "@types/mime": "2.0.3",
        "@types/mkdirp": "1.0.1",
        "@types/node": "15.0.1",
        "@typescript-eslint/eslint-plugin": "4.22.0",
        "@typescript-eslint/parser": "4.22.0",
        "ava": "3.15.0",
        "eslint": "7.25.0",
        "js-yaml": "4.1.0",
        "lint-staged": "10.5.4",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "ts-node": "9.1.1",
        "typescript": "4.2.4"
    },
    "lint-staged": {
        "*.js": [
            "eslint"
        ]
    },
    "renovate": {
        "extends": [
            ":ignoreModulesAndTests",
            ":unpublishSafe",
            ":semanticPrefixChore",
            ":prNotPending",
            ":prConcurrentLimit10",
            ":prHourlyLimitNone",
            ":label(dependencies)",
            ":automergeMinor"
        ]
    }
}
